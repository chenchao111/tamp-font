:global .search_cart {
  overflow: auto;
  top: 0;
  width: 100%;
  position: absolute;
  bottom: 0;
}
:global .search_cart .search_cart_Search {
  background-image: linear-gradient(90deg, #FA9850 3%, #FF6262 100%);
  height: 174px;
  position: relative;
}
:global .search_cart .search_cart_Search .am-search {
  height: 94px;
  padding: 19px 31px;
}
:global .search_cart .search_cart_Search .am-search .am-search-input {
  background: #ebecf1;
  height: 56px;
}
:global .search_cart .search_cart_Search .am-search .am-search-input input {
  height: 56px;
}
:global .search_cart .search_cart_Search .am-search-input .am-search-synthetic-ph {
  text-align: left;
  background: #fff;
  text-indent: 1em;
  height: 56px;
  line-height: 56px;
  font-size: 12px;
}
:global .search_cart .search_cart_Search .crosswiseMeun {
  /*
        position: absolute;
        left: 0;
        right: 0;
        bottom: 0;
        top:.94px;
        */
  white-space: nowrap;
  overflow-x: auto;
  overflow-y: hidden;
  -webkit-overflow-scrolling: auto;
}
:global .search_cart .search_cart_Search .crosswiseMeun a {
  height: 80px;
  line-height: 80px;
  margin: 0 40px;
  font-size: 30px;
  color: #fff;
  cursor: pointer;
}
:global .search_cart .search_cart_Search .crosswiseMeun .active {
  position: relative;
}
:global .search_cart .search_cart_Search .crosswiseMeun .active:after {
  content: "";
  height: 10px;
  border-radius: 2px;
  background: #FEDA58;
  position: absolute;
  bottom: 22px;
  width: 130%;
  left: -15%;
}
:global .search_cart .search-cart__content {
  flex: 1;
  display: flex;
  height: calc(-70%);
}
:global .search_cart .search-cart__content .null {
  margin: 0 auto;
  text-align: center;
}
:global .search_cart .search-cart__content .null img {
  width: 20px;
  padding-top: 10px;
}
:global .search_cart .search-cart__content .null .title {
  padding-top: 10px;
  color: #999;
  font-size: 32px;
}
:global .search_cart .search-cart__content .am-list-body::before {
  height: 0;
}
:global .search_cart .search-cart__content .search-cart__content-menu {
  width: 200px;
  height: 100%;
  background: #ebecf1;
  overflow: auto;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-list-item {
  border-left: 8px solid #ebecf1;
  padding-left: 15px;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-list-content {
  font-size: 14px !important;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-sub-menu-item-selected {
  border-left: 8px solid #FF6262;
  background: #fff;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-sub-menu-item-selected .am-list-content {
  color: #200505 !important;
  color: #FF6262 !important;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-sub-menu-item-selected .am-radio-inner {
  display: none;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-flexbox .am-flexbox-item:last-child {
  background: #EDEDED;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-flexbox .am-flexbox-item:first-child .am-list .am-list-body {
  background: #ebecf1;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-flexbox .am-flexbox-item:last-child .am-list .am-list-item {
  background: #ebecf1;
}
:global .search_cart .search-cart__content .search-cart__content-menu .am-flexbox .am-flexbox-item:last-child .am-list .am-sub-menu-item-selected {
  background: #fff;
}
:global .search_cart .search-cart__content .search-cart__content-tab {
  flex: 1;
  display: flex;
  flex-flow: wrap;
  align-content: flex-start;
  background: #fff;
  overflow: auto;
}
:global .search_cart .search-cart__content .search-cart__content-tab .serch-cart__content-tab__sub {
  width: 100%;
  height: 44px;
  padding-left: 80px;
  position: relative;
}
:global .search_cart .search-cart__content .search-cart__content-tab .serch-cart__content-tab__sub .sub-number {
  height: 30px;
  width: 30px;
  background: #333;
  position: absolute;
  left: 30px;
  top: 50%;
  transform: translateY(-50%);
  color: #999;
  text-align: center;
  background: url(https://res.zudeapp.com/recycle/recycleH5_24.png) no-repeat center;
  background-size: .3px .3px;
  line-height: .31px;
  font-family: "DINPro-Bold";
}
:global .search_cart .search-cart__content .search-cart__content-tab .serch-cart__content-tab__sub .sub-numberBg01 {
  background: url(https://res.zudeapp.com/recycle/recycleH5_21.png) no-repeat center;
  background-size: .3px .3px;
  color: #fff;
}
:global .search_cart .search-cart__content .search-cart__content-tab .serch-cart__content-tab__sub .sub-numberBg02 {
  background: url(https://res.zudeapp.com/recycle/recycleH5_22.png) no-repeat center;
  background-size: .3px .3px;
  color: #fff;
}
:global .search_cart .search-cart__content .search-cart__content-tab .serch-cart__content-tab__sub .sub-numberBg03 {
  background: url(https://res.zudeapp.com/recycle/recycleH5_23.png) no-repeat center;
  background-size: .3px .3px;
  color: #fff;
}
:global .search_cart .search-cart__content .search-cart__content-tab .serch-cart__content-tab__sub .sub-name {
  text-align: left;
  height: 100%;
  line-height: 44px;
}
